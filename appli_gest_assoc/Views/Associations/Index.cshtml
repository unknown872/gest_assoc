@model IEnumerable<appli_gest_assoc.Models.Association>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomAssociation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AnneeCreation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tel1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tel2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Adresse)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomAssociation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AnneeCreation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tel1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tel2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Adresse)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdAssociation">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdAssociation">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdAssociation">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
