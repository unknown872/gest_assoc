@model IEnumerable<appli_gest_assoc.Models.Bureau>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomBureau)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateCreation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Activite)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Association)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomBureau)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateCreation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Activite.Libelle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Association.NomAssociation)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdBureau">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdBureau">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdBureau">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
